{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "ignorePaths": [],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "enabled": true,
      "automerge": true,
      "automergeType": "branch",
      "platformAutomerge": true,
      "dependencyDashboardApproval": false
    }
  ]
}
