{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "git": {
        "pushToBranch": {
          "enabled": true,
          "createPullRequest": false,
          "pushToMain": true,
          "pushWhenGreen": true
        }
      }
    }
  ]
}
