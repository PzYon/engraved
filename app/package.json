    "@mui/system": "7.3.6",
    "@mui/x-date-pickers": "8.22.0",
    "@tanstack/react-query": "5.90.12",
    "@tanstack/react-query-devtools": "5.91.1",
    "@tiptap/markdown": "3.13.0",
    "@tiptap/pm": "3.13.0",
    "@tiptap/react": "3.13.0",
    "@tiptap/starter-kit": "3.13.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.15",
    "chart.js": "4.5.1",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.1.0",
    "chrono-node": "2.9.0",
    "date-fns": "4.1.0",
    "emoji-picker-react": "4.16.1",
    "emoji-regex": "10.6.0",
    "marked": "17.0.1",
    "react": "19.2.2",
    "react-chartjs-2": "5.3.1",
    "react-dom": "19.2.2",
    "react-hotkeys-hook": "5.2.1",
    "react-onesignal": "3.4.0",
    "react-router": "7.10.1",
    "react-router-dom": "7.10.1",
    "vite": "7.2.7"
  },
  "scripts": {
    "start": "vite",
    "api:start": "dotnet run --project ../api/Engraved.Api/",
    "build": "vite build",
    "build:e2e": "vite build -m development",
    "build:visualize-bundle": "npx vite-bundle-visualizer",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "types:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\""
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,md,json}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@tanstack/eslint-plugin-query": "5.91.2",
    "@types/node": "24.10.3",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "@vitest/ui": "4.0.15",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "eslint": "9.39.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "jsdom": "27.3.0",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "vite-bundle-visualizer": "1.2.1",
    "vite-plugin-checker": "0.12.0",
    "vitest": "4.0.15"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.53.3"
  }
}
