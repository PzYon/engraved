    "@mui/icons-material": "7.3.1",
    "@mui/material": "7.3.1",
    "@mui/x-date-pickers": "8.10.2",
    "@tanstack/react-query": "5.85.5",
    "@tanstack/react-query-devtools": "5.85.5",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "@uiw/react-codemirror": "4.25.1",
    "@vitejs/plugin-react": "5.0.1",
    "chart.js": "4.5.0",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.1.0",
    "chrono-node": "2.8.4",
    "date-fns": "4.1.0",
    "emoji-picker-react": "^4.9.4",
    "markdown-it": "14.1.0",
    "react": "19.1.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.1",
    "react-hotkeys-hook": "5.1.0",
    "react-onesignal": "3.2.3",
    "react-router-dom": "7.8.2",
    "vite": "7.1.3"
  },
  "scripts": {
    "start": "vite",
    "api:start": "dotnet run --project ../api/Engraved.Api/",
    "build": "vite build",
    "build:e2e": "vite build -m development",
    "build:visualize-bundle": "npx vite-bundle-visualizer",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "types:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "knip": "knip"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,md,json}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.34.0",
    "@types/markdown-it": "14.1.2",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "@vitest/ui": "3.2.4",
    "eslint": "9.34.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "knip": "5.63.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite-plugin-checker": "0.10.2",
    "vitest": "3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.48.0"
  }
}
