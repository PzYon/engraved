{
  "name": "engraved-app",
  "version": "0.1.0",
  "author": "PzYon",
  "type": "module",
  "dependencies": {
    "@codemirror/commands": "6.8.1",
    "@codemirror/lang-markdown": "6.3.4",
    "@codemirror/view": "6.38.3",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/styled": "11.14.1",
    "@microsoft/applicationinsights-web": "3.3.10",
    "@mui/icons-material": "7.3.2",
    "@mui/material": "7.3.2",
    "@mui/styled-engine": "7.3.2",
    "@mui/system": "7.3.2",
    "@mui/x-date-pickers": "8.11.3",
    "@tanstack/react-query": "5.89.0",
    "@tanstack/react-query-devtools": "5.89.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@uiw/react-codemirror": "4.25.2",
    "@vitejs/plugin-react": "5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "chart.js": "4.5.0",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.1.0",
    "chrono-node": "2.9.0",
    "date-fns": "4.1.0",
    "emoji-picker-react": "^4.9.4",
    "markdown-it": "14.1.0",
    "react": "19.1.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.1",
    "react-hotkeys-hook": "5.1.0",
    "react-onesignal": "3.4.0",
    "react-router": "7.9.1",
    "react-router-dom": "7.9.1",
    "vite": "7.1.7"
  },
  "scripts": {
    "start": "vite",
    "api:start": "dotnet run --project ../api/Engraved.Api/",
    "build": "vite build",
    "build:e2e": "vite build -m development",
    "build:visualize-bundle": "npx vite-bundle-visualizer",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "types:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\""
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,md,json}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.36.0",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@types/markdown-it": "14.1.2",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitest/ui": "3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "9.36.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.21",
    "globals": "16.4.0",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-checker": "0.10.3",
    "vitest": "3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.2"
  }
}
